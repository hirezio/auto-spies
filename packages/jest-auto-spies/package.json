    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "14.6.4",
    "jest": "27.0.6",
    "rxjs": "^6.0.0",
    "ts-jest": "27.0.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@hirez_io/auto-spies-core": "1.6.4"
  },
  "peerDependencies": {
    "jest": "< 28.x",
    "rxjs": ">=6.0.0 < 8",
    "typescript": ">=2.8.1"
  }
}
