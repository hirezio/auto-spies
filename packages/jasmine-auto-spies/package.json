{
  "name": "jasmine-auto-spies",
  "version": "8.0.1",
  "author": {
    "name": "Shai Reznik",
    "company": "HiRez.io"
  },
  "description": "Create automatic spies from classes in jasmine tests, also for promises and observables",
  "license": "MIT",
  "keywords": [
    "jasmine",
    "jasmine-given",
    "spies",
    "mocks",
    "async methods",
    "TypeScript Tests",
    "RxJS testing",
    "Promises tests"
  ],
  "homepage": "https://github.com/hirezio/auto-spies/tree/master/packages/jasmine-auto-spies",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirezio/auto-spies",
    "directory": "packages/jasmine-auto-spies"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.build.json",
    "build": "run-s clean compile",
    "test": "karma start --browsers ChromeHeadless",
    "test:watch": "karma start --auto-watch --no-single-run",
    "contributors:add": "all-contributors add"
  },
  "devDependencies": {
    "@types/jasmine": "^3.10.2",
    "@types/node": "18.15.5",
    "jasmine-core": "5.1.1",
    "rxjs": "^7.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@hirez_io/auto-spies-core": "3.0.0"
  },
  "peerDependencies": {
    "jasmine-core": ">=2.6.0 < 6",
    "rxjs": ">=6.0.0 < 8",
    "typescript": ">=2.8.1"
  }
}
